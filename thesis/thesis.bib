@Article{SchröderUte2008CitT,
  author    = {Schröder, Ute},
  title     = {Challenges in the Traceability of Seafood},
  journal   = {Journal für Verbraucherschutz und Lebensmittelsicherheit},
  year      = {2008},
  language  = {eng},
  volume    = {3},
  number    = {1},
  pages     = {45--48},
  issn      = {1661-5751},
  abstract  = {The increasing global trade of foods and animal feeds have to be channelled in right directions to guarantee product safety, transparency and the protection of consumer health. According to this fact the European Regulation (EC) No 178/2002 lays down the common principle for implementing traceability systems in product supply chains. However, seafood is a highly perishable food item which is increasingly global traded so that a lot of particular conditions and difficulties more have to be taken into account compared to other food products. This review discusses the relevant regulations for traceability in seafood and gives a survey about the most important analysis techniques for characterizing seafood. Der zunehmende globale Handel von Lebensmitteln und Futtermitteln erfordert entsprechende rechtliche Vorgaben, sodass Produktsicherheit und Verbraucherschutz garantiert werden können. Um diesen Anforderungen Rechnung zu tragen, legt die europäische Verordnung (EC) No 178/2002 hierzu die generellen Prinzipien zur Einführung von Rückverfolgbarkeitssystemen in Produktlieferketten dar. Fischereiprodukte sind leicht verderbliche Lebensmittel, die im hohen Maß global gehandelt werden, und daher besondere Beachtung bezüglich spezieller Bedingungen und Schwierigkeiten finden müssen. Dieser Übersichtsartikel diskutiert die relevante Gesetzgebung in Bezug auf Rückverfolgbarkeit von Fischereiprodukten und gibt einen Überblick über die wichtigsten Analysenmethoden, die sich mit dieser Problematik beschäftigen.},
  address   = {Basel},
  keywords  = {Rückverfolgbarkeit ; traceability ; seafood safety ; labelling},
  publisher = {SP Birkhäuser Verlag Basel},
}

